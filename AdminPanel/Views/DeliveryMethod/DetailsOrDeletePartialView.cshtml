    <label asp-for="DescriptionArabic" class="col-form-label"></label>
    <input asp-for="DescriptionArabic" class="form-control" disabled />
</div>


<div class="form-group">
    <label asp-for="Cost" class="col-form-label"></label>
    <input asp-for="Cost" class="form-control" disabled />
